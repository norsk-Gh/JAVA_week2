package com.javalec.base;

public class Main {

	public static void main(String[] args) {
		// 초기값이 없는 배열 선언 및 데이터 추가하기
		
		String[] str = new String[7];
		
		str[0] = "Sun";
		str[1] = "Mon";
		str[2] = "Tue";
		str[3] = "Wed";
		str[4] = "Thr";
		str[5] = "Fri";
		str[6] = "Sat";
		
		System.out.print(str);

	}

}
